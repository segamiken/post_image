<h1>New Post</h1>

<%= form_with(model: @post, local: true, multipart: true) do |f| %>

  <div>
    <%= f.label :title %>
    <%= f.text_field :title %>
  </div>

  <div>
    <%= f.label :photo %>
    <%= f.file_field :photo %>
  </div>

  <br>
  <div>
    <%= f.submit %>
  </div>
<% end %>